<!DOCTYPE html>
<html>

<head>
  <%- include('../partials/head') %>
</head>

<body class="container">
  <header>
    <%- include('../partials/header') %>
  </header>

  <main>
    <div class="jumbotron">

      <h2>Quotes!</h2>

      <h1>All the quotes</h1>
      <ul>
        <% quotes.forEach(function(quote) { %>
          <li>
            <%= quote.name %> -
            <%= quote.quote %>
          </li>
        <% }); %> 
      </ul>

    </div>
  </main>


  <footer>
    <%- include('../partials/footer') %>
  </footer>

</body>

</html>
